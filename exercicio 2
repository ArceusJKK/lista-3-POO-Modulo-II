    ArrayList<Heroi> Herois = new ArrayList<>();


  ArrayList<Inimigo> getInimigoMaiorAtaque() {
      Inimigo ret = new Inimigo();

      for (int i = 0; i > inimigos.size(); i++) {
          if (inimigos.get(i).ataqueInimigo > ret.ataqueInimigo){
          }
      }
      return inimigos;
  }
}
